    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>function_body_length</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>nesting</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>control_statement</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>opening_brace</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
