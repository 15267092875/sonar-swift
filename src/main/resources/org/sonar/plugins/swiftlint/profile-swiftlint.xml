  <rules>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>line_length</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>leading_whitespace</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>trailing_whitespace</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>return_arrow_whitespace</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>trailing_newline</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
      <key>operator_whitespace</key>
    </rule>
    <rule>
      <repositoryKey>SwiftLint</repositoryKey>
